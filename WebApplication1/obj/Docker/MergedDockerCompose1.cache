services:
  db:
    container_name: db
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Atlo2DrP1
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1433
      target: 1433
  webapplication1:
    build:
      context: C:\Users\zepho\source\repos\WebApplication1
      dockerfile: WebApplication1/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: WebApplication1
      target: base
    container_name: webapplication1
    depends_on:
      db:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:5000
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: webapplication1:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/WebApplication1.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5000
      target: 5000
    - target: 5000
    tty: true
    volumes:
    - C:\Users\zepho\source\repos\WebApplication1\WebApplication1:/app:rw
    - C:\Users\zepho\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\zepho\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\zepho\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\zepho\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\zepho\source\repos\WebApplication1:/src:rw
version: '3.4'